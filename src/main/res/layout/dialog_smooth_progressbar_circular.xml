<?xml version="1.0" encoding="utf-8"?>
<!-- smooth progressbar needs -->
<com.handsomeyang.mango.thrid.smoothprogressbar.circular.CircularProgressBar
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/circular_smooth_progressbar"
    android:layout_width="60dp"
    android:layout_height="60dp"
    android:layout_centerInParent="true"
    android:indeterminate="true"
    android:padding="5dp"
    app:cpb_color="#FFee44"
    app:cpb_colors="@array/mycolors"
    app:cpb_max_sweep_angle="300"
    app:cpb_min_sweep_angle="10"
    app:cpb_rotation_speed="1.2"
    app:cpb_stroke_width="5dp"
    app:cpb_sweep_speed="1.5"
    >
</com.handsomeyang.mango.thrid.smoothprogressbar.circular.CircularProgressBar>
